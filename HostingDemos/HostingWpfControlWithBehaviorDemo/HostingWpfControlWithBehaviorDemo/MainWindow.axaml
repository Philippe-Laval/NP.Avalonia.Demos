<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		Width="1000"
		Height="600"
        x:Class="HostingWpfControlWithBehaviorDemo.MainWindow"
        Title="HostingWpfControlWithBehaviorDemo"
		xmlns:local="clr-namespace:HostingWpfControlWithBehaviorDemo"
		xmlns:beh="clr-namespace:BehaviorLibrary;assembly=BehaviorLibrary"
		xmlns:wpf="clr-namespace:WpfControlToHost;assembly=WpfControlToHost">
	<Grid x:Name="RootPanel">
		<beh:ViewEmbeddingControl x:Name="EmbeddingControl">
			<wpf:MyWpfUserControl/>
		</beh:ViewEmbeddingControl>
	</Grid>
</Window>

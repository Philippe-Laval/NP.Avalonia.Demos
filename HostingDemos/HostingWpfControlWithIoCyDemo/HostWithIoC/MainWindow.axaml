<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:winVisuals="clr-namespace:WinVisuals;assembly=WinVisuals"
		Width="800"
		Height="600"
        x:Class="HostWithIoC.MainWindow"
        Title="HostWithIoC">
	<Grid>
		<winVisuals:NativeEmbeddingControl x:Name="NativeEmbedder"
										   HorizontalAlignment="Stretch"
										   VerticalAlignment="Stretch">
			<winVisuals:NativeEmbeddingControl.DefaultObject>
				<Grid>
					<TextBlock Text="No implementation for this platform"
							   FontSize="24"
							   HorizontalAlignment="Center"
							   VerticalAlignment="Center"/>
				</Grid>
			</winVisuals:NativeEmbeddingControl.DefaultObject>
		</winVisuals:NativeEmbeddingControl>
	</Grid>
</Window>
